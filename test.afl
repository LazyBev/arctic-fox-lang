macro abc
    bit 0 + 97 store // a
    bit 1 + 98 store // b
    bit 2 + 99 store // c
    bit 3 + 10 store // \n
close

macro write 
    dup bit 1 1 syscall3
close

20 10 - prn // 10

20 10 + prn // 30

20 10 * prn // 200

20 10 / prn // 2

20 2 % prn // 0

20 20 + 40 = if
    20 20 + 41 = if
        50 prn
    else
        70 prn
    end
else
    60 prn
end // 70

abc 4 write // 'abc'

1 while dup 4 > do
    dup prn
    1 +
done // 1 2 3

exit
